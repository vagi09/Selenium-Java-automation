<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<listeners>

		<listener class-name="Academy.Listeners"/>
	</listeners>
	<test thread-count="5" name="Validating Banner">
		<classes>
			<class name="Academy.ValidateBanner" />
		</classes>
	</test>


	<test name="validating navigation bar">
		<classes>
			<class name="Academy.validateNavigationBar" />
		</classes>
	</test>

	<test name="validating Home page">
		<classes>
			<class name="Academy.HomePage" />
		</classes>
	</test>

	<test name="validating Title">
		<classes>
			<class name="Academy.validateTitle" />
		</classes>
	</test>


</suite> <!-- Suite -->
